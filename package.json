{
  "private": true,
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "lint-fix": "npx eslint --fix ./*.js ./app/**/*.{js,jsx,ts,tsx}",
    "lint": "npx eslint ./*.js ./app/**/*.{js,jsx,ts,tsx}",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/index.css -o ./app/styles/index.css",
    "dev": "remix build && run-p dev:*",
    "dev:css": "tailwindcss -w -i ./styles/index.css -o ./app/styles/index.css",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "deploy": "npm run build && wrangler deploy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@remix-run/cloudflare": "^2.2.0",
    "@remix-run/cloudflare-workers": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "@types/chart.js": "^2.9.40",
    "@types/common-tags": "^1.8.4",
    "@types/moment": "^2.13.0",
    "@types/react-chartjs-2": "^2.5.7",
    "autoprefixer": "^10.4.16",
    "chart.js": "^4.4.0",
    "chartjs-adapter-moment": "^1.0.1",
    "common-tags": "^1.8.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "moment": "^2.29.4",
    "octokit": "^3.1.1",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "tailwindcss": "^3.3.5",
    "wrangler": "^3.15.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.53.0",
    "eslint-config-google": "^0.14.0",
    "miniflare": "^3.20231025.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  }
}
